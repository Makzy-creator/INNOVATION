{
  "name": "innovation-blood-donation",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "compile": "npx hardhat compile",
    "deploy:mumbai": "npx hardhat run scripts/deploy.js --network mumbai",
    "deploy:polygon": "npx hardhat run scripts/deploy.js --network polygon",
    "deploy:local": "npx hardhat run scripts/deploy.js --network hardhat",
    "test:contracts": "npx hardhat test",
    "verify:mumbai": "npx hardhat verify --network mumbai"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "date-fns": "^2.30.0",
    "ethers": "^6.8.0",
    "framer-motion": "^10.16.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-markdown": "^9.0.1",
    "react-player": "^2.13.0",
    "react-router-dom": "^6.15.0",
    "react-webcam": "^7.1.1",
    "remark-gfm": "^4.0.0",
    "socket.io-client": "^4.7.2"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^3.0.2",
    "@openzeppelin/contracts": "^4.9.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "dotenv": "^16.3.1",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "hardhat": "^2.17.2",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}